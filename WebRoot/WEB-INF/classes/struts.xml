			<result name="fail">/fail.jsp</result>
			<result name="cvUpdate">/updateCV.jsp</result>
		</action>
		
		<action name="deleteCV" class="cv" method="delete">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="deleteCV">/deleteCV.jsp</result>
		</action>
		
