<struts>
	<package name="recruitment" extends="struts-default">
		<global-results>
			<result name="Exception">/common/exception.jsp</result>
		</global-results>

		<global-exception-mappings>
			<exception-mapping result="Exception"
				exception="java.lang.RuntimeException"></exception-mapping>
		</global-exception-mappings>
		
		<action name="job1" class="job" method="logged">
			<result name="logged">/welcome.jsp</result>
		</action>

		<action name="js" class="js">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="load">/preUpdateJs.jsp</result>
			<result name="get">/jsProfile.jsp</result>
			<result name="list">listJobSeeker.jsp</result>
			<result name="logout" type="chain">job1</result>
			<result name="registerJs">/addJs.jsp</result>
		</action>
		
		<action name="searchJsByEmail" class="js" >
			<result name="loadByEmail">/preLoginJs.jsp</result>
		</action>

		<action name="job2" class="job" method="logged">
			<result name="logged">/jsLogged.jsp</result>
		</action>
				
		<action name="jsLogin" class="js" method="isLogin">
			<result name="success" type="chain">job2</result>
			<result name="fail">/fail.jsp</result>
			<result name="login">/loginJs.jsp</result>
		</action>
				
		<action name="jsCheckUsername" class="js" method="checkUsernameExist"/>
		<action name="jsCheckEmail" class="js" method="checkEmailExist"/>
		
		<action name="job1" class="job" method="logout">
			<result name="logout">/welcome.jsp</result>
		</action>
		
		<action name="jsLogout" class="js" method="logout">
			<result name="logout" type="chain">job1</result>
			<result name="fail">/fail.jsp</result>
			<result name="success">/success.jsp</result>
		</action>
		
		<action name="updateJs" class="js" method="updateJs">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="update">/updateJs.jsp</result>
		</action>
		
		<action name="updateJsPassword" class="js" method="updatePassword">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
		</action>

		<action name="deleteJs" class="js" method="delete">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="delete">/deleteJs.jsp</result>
		</action>

		<action name="emp" class="emp">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="list">/listEmployer.jsp</result>
			<result name="load">/preUpdateEmp.jsp</result>
			<result name="get">/empProfile.jsp</result>
			<result name="logout" type="chain">job1</result>
		</action>
		
		<action name="empCheckUsername" class="emp" method="checkUsernameExist"/>
		<action name="empCheckEmail" class="emp" method="checkEmailExist"/>

		<action name="searchEmp" class="emp" method="loadById">
			<result name="load">/searchEmpById.jsp</result>
		</action>
		<!-- 
		<action name="empLog" class="js" method="listByJc">
			<result name="listByJc">/listJobSeeker.jsp</result>
		</action>
		-->
		<action name="empLog" class="js" method="listByJc">
			<result name="listByJc">/listJobSeeker.jsp</result>
		</action>
		
		<action name="empLogin" class="emp" method="isLogin">
			<result name="success" type="chain">empLog</result>
			<result name="fail">/fail.jsp</result>
			<result name="login">/loginEmp.jsp</result>
		</action>

		<action name="deleteEmp" class="emp" method="delete">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="delete">/deleteEmp.jsp</result>
		</action>

		<action name="updateEmp" class="emp" method="updateEmp">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="update">/updateEmp.jsp</result>
		</action>
		
		<action name="updateEmpPassword" class="emp" method="updatePassword">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
		</action>
		
		<action name="admin" class="admin" method="logout">
			<result name="logout">/adminLogin.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="success">/success.jsp</result>
		</action>

		<action name="adminLogin" class="admin" method="isLogin">
			<result name="success">/userManager/admin.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="login">/userManager/adminLogin.jsp</result>
