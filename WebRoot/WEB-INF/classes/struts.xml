			<result name="logged">/jsLogged.jsp</result>
			<result name="delete">/deleteJob.jsp</result>
			<result name="update">/preUpdate.jsp</result>
			<result name="load">/preUpdateJob.jsp</result>
			<result name="registerJob">/addJob.jsp</result>
