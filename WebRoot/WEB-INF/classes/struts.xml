			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="update">/updateJob.jsp</result>
		</action>
		
		<action name="deleteJob" class="job" method="delete">
			<result name="success">/success.jsp</result>
			<result name="fail">/fail.jsp</result>
			<result name="delete">/deleteJob.jsp</result>
		</action>
		
